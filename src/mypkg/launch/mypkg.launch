<launch>
  <env name="ROSCONSOLE_CONFIG_FILE" value="$(find mypkg)/launch/rosconsole.conf"/>  
  <env name="ROSCONSOLE_FORMAT" value="[${severity}] [${time}] [${node}]: ${message}"/>
  <node name="mypkg" pkg="mypkg" type="mypkg" output="screen" >
    <param name="db_name" value="/test10.db"/>
  </node>
</launch>